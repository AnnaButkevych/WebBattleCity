@model GameBoardViewModel

@{
    ViewData["Title"] = "Home Page";
}

@for (int i = 0; i < Model.Matrix.GetLength(0); i++)
    {
        for (int j = 0; j < Model.Matrix.GetLength(1); j++)
        {
            <div>
                <img src="@Url.Content("~/Images/" + Model.Matrix[i, j])" alt="Picture" />
            </div>
        }
    }       